{
  "name": "prices-notifier",
  "productName": "prices-notifier",
  "version": "1.0.0",
  "description": "Application to show notifications for different prices and a configurable minimum to show",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "prepare": "husky install",
    "postinstall": "husky install"
  },
  "keywords": [],
  "author": {
    "name": "Prince Hernandez",
    "email": "princezhm@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./electron/assets/tray_icon.png"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "prices_notifier"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/plugin-webpack": "6.0.0-beta.54",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/better-sqlite3": "^5.4.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "bindings": "^1.5.0",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^4.2.1",
    "electron": "12.0.5",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "husky": "^6.0.0",
    "node-loader": "^1.0.1",
    "prettier": "^2.2.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "typescript": "^4.0.2",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/data-grid": "^4.0.0-alpha.26",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@types/lodash": "^4.14.168",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.1",
    "better-sqlite3": "^7.1.5",
    "electron-main-fetch": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "eslint-plugin-react": "^7.23.2",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "node-pre-gyp": "^0.17.0",
    "notistack": "^1.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2"
  }
}
